# Copy to terraform.tfvars and fill in secrets locally (never commit the real file)

# --- Mode toggle ---
enable_public_rds  = true
enable_private_rds = false

# --- AWS/App names ---
aws_region    = "ap-northeast-1"
app_name      = "warehouse-optimizer-backend"
ecr_repo_name = "warehouse-optimizer-backend"

# --- Database ---
rds_db_name          = "warehouse"
rds_master_username  = "appuser"
# Replace with your real password locally
rds_master_password  = "CHANGE_ME"
