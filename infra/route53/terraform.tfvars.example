# Copy this file to terraform.tfvars and fill in the values

# AWS Region
aws_region = "ap-northeast-1"

# Domain name (register this domain first at a domain registrar)
domain_name = "warehouse-optimizer.com"

# App Runner service ARN (from infra/apprunner output)
# Run: cd ../apprunner && terraform output apprunner_service_arn
apprunner_service_arn = "arn:aws:apprunner:ap-northeast-1:904368995258:service/warehouse-optimizer-backend/c75ca38ef67148e0bced484f9a2569be"

# App Runner default domain (from infra/apprunner output)
# Run: cd ../apprunner && terraform output apprunner_service_url
apprunner_default_domain = "cdcdwjbjms.ap-northeast-1.awsapprunner.com"
